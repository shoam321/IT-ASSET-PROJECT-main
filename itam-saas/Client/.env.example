# Copy to .env for local development
# In Vercel: Project Settings -> Environment Variables

# Backend API base URL (must include /api)
REACT_APP_API_URL=

# Grafana base URL (no trailing slash preferred)
REACT_APP_GRAFANA_URL=

# PayPal client ID (public identifier, still set via env)
REACT_APP_PAYPAL_CLIENT_ID=

# PayPal monthly subscription Plan ID (Regular)
REACT_APP_PAYPAL_REGULAR_PLAN_ID=
