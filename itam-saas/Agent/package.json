{
  "name": "itam-tracker-server",
  "version": "1.0.0",
  "type": "module",
  "description": "IT Asset Tracker Backend Server - Node.js with PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node migrations/run.js",
    "migrate:all": "node migrations/add-cost-column.js && node migrations/run-multi-tenancy-migration.js && node migrations/run-audit-migration.js && node migrations/run-receipts-migration.js && node migrations/run-google-sso-migration.js && node migrations/run-fix-devices-ownership-claim.js",
    "migrate:user-asset-ownership": "node run-user-asset-ownership-migration.js",
    "test:assets-isolation": "node tests/assets-isolation-smoke.js"
  },
  "dependencies": {
    "@xyflow/react": "^12.10.0",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "playwright": "^1.57.0",
    "redis": "^5.10.0",
    "resend": "^6.6.0",
    "socket.io": "^4.8.3",
    "tesseract.js": "^5.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
